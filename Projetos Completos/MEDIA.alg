algoritmo "semnome"
// Função :
// Autor :
// Data : 10/06/2024
// Seção de Declarações 
var
 n1, n2, m : real
inicio

      Escreval ("Digite um número")
      leia (n1)
      escreval("Digite outro número")
      leia (n2)
      m <- (n1+ n2)/2
      Escreval ("A média  de ", n1, " e ", n2, " é igual à: ", m)
fimalgoritmo
