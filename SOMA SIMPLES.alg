algoritmo "Exercicio soma"
// Função :
// Autor :
// Data : 10/06/2024
// Seção de Declarações
var
 n1, n2, s : real
inicio
      escreval ("Vamos somar 2 números")
      Escreval ("Digite um número")
      leia (n1)
      escreval("Digite outro número")
      leia (n2)
      s <- n1+ n2
      Escreval ("A Soma de ", n1, " e ", n2, " é igual à: ", s)
fimalgoritmo
